{
  "version": 2,
  "builds": [
    {
      "src": "./unith_con.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "unith_con.py"
    }
  ],
  {
  "functions": {
    "./**/*": {
      "maxDuration": 60 // All functions can run for a maximum of 5 seconds
    }
  }
}
}
